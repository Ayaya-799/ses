<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

   <web-app id="WebApp">
      <display-name>icbccmisTest</display-name>
      <servlet>
         <servlet-name>TestConnPool</servlet-name>
         <display-name>TestConnPool</display-name>
         <servlet-class>icbc.cmis.servlets.TestConnPool</servlet-class>
      </servlet>
      <servlet>
         <servlet-name>CmisStartupServlet</servlet-name>
         <display-name>CmisStartupServlet</display-name>
         <servlet-class>icbc.cmis.servlets.CmisStartupServlet</servlet-class>
         <init-param>
            <param-name>initStart</param-name>
            <param-value>true</param-value>
         </init-param>
         <init-param>
            <param-name>CTEFilePath</param-name>
            <param-value>D:\eclipse\eclipse-jee-juno-win32\workspace\wasau\WebContent\config\icbccmis.xml</param-value>
         </init-param>
         <init-param>
            <param-name>userName</param-name>
            <param-value>icbc</param-value>
         </init-param>
         <init-param>
            <param-name>userPass</param-name>
            <param-value>icbc</param-value>
         </init-param>
         <init-param>
            <param-name>JVMID</param-name>
            <param-value>1</param-value>
         </init-param>
         <init-param>
            <param-name>appCode</param-name>
            <param-value>3</param-value>
         </init-param>
         <init-param>
            <param-name>dseIniPath</param-name>
            <param-value>D:\eclipse\eclipse-jee-juno-win32\workspace\wasau\WebContent\server\dse.ini</param-value>
         </init-param>
         <load-on-startup>1</load-on-startup>
      </servlet>
      <servlet-mapping>
         <servlet-name>TestConnPool</servlet-name>
         <url-pattern>/TestConnPool</url-pattern>
      </servlet-mapping>
      <servlet>
         <servlet-name>CmisSessionServlet</servlet-name>
         <display-name>CmisSessionServlet</display-name>
         <servlet-class>icbc.cmis.servlets.CmisSessionServlet</servlet-class>
      </servlet>
      <servlet-mapping>
         <servlet-name>CmisSessionServlet</servlet-name>
         <url-pattern>/servlet/icbc.cmis.servlets.CmisSessionServlet</url-pattern>
      </servlet-mapping>
      <servlet>
         <servlet-name>CmisReqServlet</servlet-name>
         <display-name>CmisReqServlet</display-name>
         <servlet-class>icbc.cmis.servlets.CmisReqServlet</servlet-class>
      </servlet>
      <servlet-mapping>
         <servlet-name>CmisReqServlet</servlet-name>
         <url-pattern>/servlet/icbc.cmis.servlets.CmisReqServlet</url-pattern>
      </servlet-mapping>
      <welcome-file-list>
         <welcome-file>index.html</welcome-file>
         <welcome-file>index.htm</welcome-file>
         <welcome-file>index.jsp</welcome-file>
         <welcome-file>default.html</welcome-file>
         <welcome-file>default.htm</welcome-file>
         <welcome-file>default.jsp</welcome-file>
      </welcome-file-list>
      <taglib>
         <taglib-uri>cmisTags</taglib-uri>
         <taglib-location>/WEB-INF/taglib/cmisTagLib.tld</taglib-location>
      </taglib>
      <taglib>
         <taglib-uri>CTPTLD</taglib-uri>
         <taglib-location>/WEB-INF/taglib/CTPTaglib.tld</taglib-location>
      </taglib>
      <taglib>
         <taglib-uri>waftags</taglib-uri>
         <taglib-location>/WEB-INF/taglib/waftags.tld</taglib-location>
      </taglib>
      <resource-ref>
     	<description>CMISDB</description>
		<res-ref-name>jdbc/CMISDB</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	  </resource-ref>
      <resource-ref>
     	<description>CMISDBM</description>
		<res-ref-name>jdbc/CMISDBM</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	  </resource-ref>
   </web-app>
